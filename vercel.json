  {
    "name": "symfony-demo",
    "version": 2,
    "builds": [
      { "src": "public/index.php", "use": "vercel-php" },
      { "src": "public/!(*.php)", "use": "@vercel/static" },
      { "src": "public/**/!(*.php)", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/build/(.*)", "dest": "public/build/$1"},
      { "src": "/(.*)", "dest": "public/index.php" } 
    ],
    "public": true,
    "scope": "ties"
  }